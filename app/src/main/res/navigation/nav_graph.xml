            android:defaultValue="NA" />
        <argument
            android:name="lecture4"
            app:argType="string"
            android:defaultValue="NA" />
        <argument
            android:name="lecture5"
            app:argType="string"
            android:defaultValue="NA" />
        <argument
            android:name="lecture6"
            app:argType="string"
            android:defaultValue="NA" />
        <argument
            android:name="lecture7"
            app:argType="string"
            android:defaultValue="NA" />
        <argument
            android:name="lecture8"
            app:argType="string"
            android:defaultValue="NA" />
    </fragment>


    <fragment
        android:id="@+id/payment2"
