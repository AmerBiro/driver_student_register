    <fragment
        android:id="@+id/student_info"
        android:name="com.example.driverstudentregister.student.Info"
        android:label="student_info"
        tools:layout="@layout/student_info" >

        <argument
            android:name="position"
            app:argType="integer"
            android:defaultValue="0" />
        <argument
            android:name="name"
            app:argType="string"
            android:defaultValue="NA" />
        <argument
            android:name="phone"
            app:argType="string"
            android:defaultValue="NA" />
        <argument android:name="street"
            app:argType="string"
            android:defaultValue="NA" />
        <argument android:name="zip_code"
            app:argType="string"
            android:defaultValue="NA" />
        <argument android:name="city"
            app:argType="string"
            android:defaultValue="NA" />
        <argument
            android:name="cpr"
            app:argType="string"
            android:defaultValue="NA" />
        <argument
            android:name="studentId"
            app:argType="string"
            android:defaultValue="NA" />
        <action
            android:id="@+id/action_student_info_to_main3"
            app:destination="@id/main" />
        <action
            android:id="@+id/action_student_info_to_home27"
            app:destination="@id/home2" />
